{
    "name": "jmhc/laravel-api",
    "description": "Laravel restful api.",
    "keywords": [
        "restful",
        "api",
        "laravel"
    ],
    "homepage": "http://jumihc.com/",
    "license": "MIT",
    "authors": [
        {
            "name": "jumihc",
            "email": "jumihuchuang@126.com"
        }
    ],
    "autoload": {
        "psr-4": {
            "Jmhc\\Restful\\": "src/"
        },
        "files": [
            "src/helpers.php"
        ]
    },
    "require": {
        "php": "^7.2",
        "ext-json": "*",
        "ext-openssl": "*",
        "laravel/framework": "^5.5|^6.0",
        "doctrine/dbal": "^2.0",
        "guzzlehttp/guzzle": "^6.0",
        "jenssegers/mongodb": "^3.0",
        "vladimir-yuldashev/laravel-queue-rabbitmq": "^9.0"
    },
    "extra": {
        "laravel": {
            "providers": [
                "Jmhc\\Restful\\Providers\\JmhcApiServiceProvider",
                "Jmhc\\Restful\\Providers\\JmhcJobServiceProvider",
                "Jmhc\\Restful\\Providers\\JmhcContractServiceProvider",
                "Jmhc\\Restful\\Providers\\JmhcRouteServiceProvider"
            ]
        }
    }
}
